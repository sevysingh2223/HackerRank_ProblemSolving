    public static List<Integer> compareTriplets(List<Integer> a, List<Integer> b) {
        int m=0;
        int n=0;
        List<Integer> list = new LinkedList<Integer>();
        for(int i=0;i<3;i++)
        {
            if(a.get(i)>b.get(i))
                m++;
            else if(a.get(i)<b.get(i))
                n++;
            }
        list.add(m);
        list.add(n);
        return list;

    }

}

